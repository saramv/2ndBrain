     * @var string $nombre
     * @ORM\Column(type="string", length=255, name="nombre")
     */
    protected $nombre;
    
    /**
     * @var string $contraseÃ±a
     * @ORM\Column(type="string", length=255, name="contraseÃ±a")
     */
    protected $contraseÃ±a;

